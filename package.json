{
	"name": "fs-fixture",
	"version": "0.0.0-semantic-release",
	"description": "Easily create test fixtures at a temporary file-system path",
	"keywords": [
		"test",
		"fixture",
		"temporary",
		"file-system",
		"disk",
		"json",
		"object",
		"template"
	],
	"license": "MIT",
	"repository": "privatenumber/fs-fixture",
	"funding": "https://github.com/privatenumber/fs-fixture?sponsor=1",
	"author": {
		"name": "Hiroki Osame",
		"email": "hiroki.osame@gmail.com"
	},
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"exports": {
		"require": {
			"types": "./dist/index.d.cts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		}
	},
	"imports": {
		"#fs-fixture": {
			"types": "./src/index.ts",
			"development": "./src/index.ts",
			"default": "./dist/index.mjs"
		}
	},
	"packageManager": "pnpm@9.11.0",
	"scripts": {
		"build": "pkgroll --minify --target=node18",
		"lint": "lintroll --node --cache .",
		"type-check": "tsc --noEmit",
		"test": "tsx tests",
		"dev": "tsx watch --conditions=development tests",
		"prepack": "pnpm build && clean-pkg-json"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"devDependencies": {
		"@types/node": "^22.7.5",
		"clean-pkg-json": "^1.2.0",
		"lintroll": "^1.10.0",
		"manten": "^1.3.0",
		"pkgroll": "^2.5.0",
		"tsx": "^4.19.1",
		"typescript": "^5.6.3"
	}
}
